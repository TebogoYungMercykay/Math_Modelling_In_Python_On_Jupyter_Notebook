   "source": [
    "def g_non_recursive(n):\n",
    "    K = [8,5,2]\n",
    "    for e in range (3, n+1):\n",
    "       K.append(8*K[e-1] + K[e-2])\n",
    "    return K[n]"
   ]
  },
