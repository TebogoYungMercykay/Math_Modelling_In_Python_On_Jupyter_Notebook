   "source": [
    "def pell_recursive(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return 2 * pell_recursive(n - 1) + pell_recursive(n - 2)"
   ]
